{
  "name": "M4A Transcriber",
  "start_url": "/",
  "display": "standalone",
  "icons": [],
  "share_target": {
    "action": "/receive/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "audio",
          "accept": ["audio/m4a", "audio/mp4", "audio/*"]
        }
      ]
    }
  }
}
